{
  "rules": {
    "indent": [1, 2],
    "quotes": [
      2, "single"
    ],
    "linebreak-style": [
      2, "unix"
    ],
    "semi": [
      2, "always"
    ],
    "no-console": 0,
    "no-unused-vars": 1
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "extends": "eslint:recommended"
}
